print("THERE ARE 3 MAJOR PARTIES TO VOTE FOR:")
print("1 for BJP")
print("2 for CONGRESS")
print("3 for AAP")

choice = int(input("Enter digit from 1/2/3: "))
age = int(input("Enter your age: "))

if age >= 18:
    print("You are eligible to vote.")
    if choice == 1:
        print("Vote goes to BJP.")
    elif choice == 2:
        print("Vote goes to Congress.")
    elif choice == 3:
        print("Vote goes to AAP.")
    else:
        print("Invalid choice. Please enter 1, 2, or 3.")
else:
    print("Not eligible to vote.")
